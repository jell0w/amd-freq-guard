{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AMDFreqGuard",
  "version": "0.1.0",
  "identifier": "com.jellow.amd-freq-guard",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AMDFreqGuard",
        "width": 900,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/kb.ico"
    ]
  }
}
